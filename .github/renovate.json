{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		":semanticPrefixFixDepsChoreOthers",
		":ignoreModulesAndTests",
		"group:all",
		"workarounds:all"
	],
	"branchConcurrentLimit": 0,
	"forkProcessing": "enabled",
	"packageRules": [
		{
			"matchManagers": ["npm"],
			"enabled": false
		},
		{
			"matchManagers": ["gomod"],
			"matchDepTypes": ["replace"],
			"enabled": false
		}
	]
}
